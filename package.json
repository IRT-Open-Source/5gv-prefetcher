{
  "name": "prefetcher",
  "version": "0.0.1",
  "description": "Part of the 5G-VICTORI media cache prefilling solution. Requests content from a caching server in order to make the caching server that content.",
  "author": "Christoph Ziegler",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/microservices": "^6.11.11",
    "@nestjs/platform-express": "^6.11.11",
    "bluebird": "^3.7.2",
    "command-line-args": "^5.1.1",
    "messenger": "git+ssh://git@gitlab.irt.de/5g-victori/messenger.git",
    "node-nats-streaming": "^0.2.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^8.8.2-beta.8",
    "@nestjs/cli": "^7.5.2",
    "@nestjs/schematics": "^7.2.1",
    "@nestjs/testing": "^6.11.11",
    "@types/bluebird": "^3.5.33",
    "@types/express": "^4.17.9",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.4",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.9.7",
    "write-json-file": "^4.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
